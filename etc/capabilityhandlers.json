[
    {
        "match": "0([1-9][0-9]*)a\\S+",
        "callback": "AuthorView_Capability::apply_old_author_view"
    },
    {
        "match": "([1-9][0-9]*)ra(\\S+)",
        "callback": "ReviewAccept_Capability::apply_review_acceptor"
    },
    {
        "match": "ra([1-9][0-9]*)([a-zA-Z]+)",
        "callback": "ReviewAccept_Capability::apply_review_acceptor"
    },
    {
        "match": "kiosk-([a-zA-Z0-9]+)",
        "callback": "MeetingTracker::apply_kiosk_capability"
    }
]
